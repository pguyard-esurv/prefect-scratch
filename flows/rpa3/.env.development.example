# RPA3 Flow - Development Environment Configuration
# This file shows how to override global database settings for specific flows

# Flow-specific database settings for RPA3's concurrent processing needs
DEVELOPMENT_RPA3_RPA_DB_POOL_SIZE=10
DEVELOPMENT_RPA3_RPA_DB_MAX_OVERFLOW=20
DEVELOPMENT_RPA3_RPA_DB_TIMEOUT=30

# Optimized settings for SurveyHub read operations
DEVELOPMENT_RPA3_SURVEYHUB_POOL_SIZE=5
DEVELOPMENT_RPA3_SURVEYHUB_MAX_OVERFLOW=10
DEVELOPMENT_RPA3_SURVEYHUB_TIMEOUT=45

# Aggressive retry settings for RPA3's high-throughput processing
DEVELOPMENT_RPA3_DATABASE_RETRY_ATTEMPTS=5
DEVELOPMENT_RPA3_DATABASE_RETRY_MIN_WAIT=0.5
DEVELOPMENT_RPA3_DATABASE_RETRY_MAX_WAIT=10

# Flow-specific distributed processing settings for RPA3 (high-throughput)
DEVELOPMENT_RPA3_DISTRIBUTED_PROCESSOR_DEFAULT_BATCH_SIZE=150
DEVELOPMENT_RPA3_DISTRIBUTED_PROCESSOR_CLEANUP_TIMEOUT_HOURS=1
DEVELOPMENT_RPA3_DISTRIBUTED_PROCESSOR_MAX_RETRIES=3
DEVELOPMENT_RPA3_DISTRIBUTED_PROCESSOR_ENABLED=true

# Copy this file to .env.development to activate flow-specific settings
# cp .env.development.example .env.development